{
    "name": "fecs",
    "displayName": "fecs",
    "description": "fecs extension for vscode",
    "icon": "fecs.jpg",
    "version": "0.6.2",
    "publisher": "Marx",
    "homepage": "https://github.com/MarxJiao/VScode-fecs",
    "bugs": "https://github.com/MarxJiao/VScode-fecs/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/MarxJiao/VScode-fecs"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Linters"
    ],
    "activationEvents": [
        "onCommand:extension.fecs"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.fecs",
                "title": "fecs"
            }
        ],
        "keybindings": [
            {
                "command": "extension.fecs",
                "key": "cmd+shift+r"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "fecs configuration",
            "properties": {
                "fecs.en": {
                    "type": "boolean",
                    "default": false,
                    "description": "false: use English in output,true: use Chinese in output"
                },
                "fecs.auto": {
                    "type": "boolean",
                    "default": false,
                    "description": "true for auto use fecs on save, false for not"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}